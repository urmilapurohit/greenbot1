
@using FormBot.Helper;



<div class="panel" name="BaselineEquipment" data-panelid="@Model.BaselineEquipmentId">
    <div class="schedule-heading panel-heading" id="headingOne">
        <div class="visit-id min-w-0 w-130">
            <label> Calc Zone:</label>
            <span class="color-text">@Model.CalcZone</span>
        </div>
        <div class="min-w-0 w-220">
            <label>Area Name :</label>
            <span class="color-text">@Model.VEECAreaName</span>
        </div>
        <div class="min-w-0 w-130">
            <label>Quantity :</label>
            <span class="color-text">@Model.Quantity</span>
        </div>
        <div class="min-w-0 w-220">
            <label>Nominal Lamp Power :</label>
            <span class="color-text">@Model.NominalLampPower</span>
        </div>
        <div>
            <label>Space-Type:</label>
            <span class="color-text">@Model.SpaceTypeName</span>
        </div>
        <div class="panel-title">
            <ul class="schedule-option">
                <li>
                    <a class="expand-btn" data-toggle="collapse" data-parent="#schedule" href="#@Model.BaselineEquipmentId" aria-expanded="true" aria-controls="collapseOne">
                        <span class="icon expand-view sprite-img">
                        </span>Expand View
                    </a>
                </li>
                <li>
                    @{var serializer = new System.Web.Script.Serialization.JavaScriptSerializer();
                        var BaselineUpgrade = TempData["BaselineUpgrade"]; }

                    @*<a href="#" title="Edit" onclick="showBaselineUpgradeEquipmentDetail('@serializer.Serialize(Model)',@BaselineUpgrade,event)">Edit</a>
                    <a href="#" title="Delete" onclick="deleteBaselineUpgradeEquipmentDetail('@Model.BaselineEquipmentId',@BaselineUpgrade,event)">Delete</a>*@

                    <a href="#" onclick="showBaselineUpgradeEquipmentDetail('@serializer.Serialize(Model)',@BaselineUpgrade,event)" class="action edit" style="background:url(/Images/edit-icon.png) no-repeat center; text-decoration:none;" title="Edit">                    </a>
                    <a href="#" onclick="deleteBaselineUpgradeEquipmentDetail('@Model.BaselineEquipmentId',@BaselineUpgrade,event)" class="action delete" style="background:url(/Images/delete-icon.png) no-repeat center; text-decoration:none;" title="Delete"></a>



                </li>
            </ul>
        </div>
    </div>
    <div class="panel-collapse collapse" id="@Model.BaselineEquipmentId" role="tabpanel" aria-labelledby="headingOne">

        <div class="row schedule-installer">
            @*<div class="col-md-4">
                    <div>
                    Calc Zone:<span class="color-text">@lstBaseline.CalcZone</span>
                    </div>
                </div>*@

            <div class="col-md-6">
                <div>
                    <label>SpaceType:</label>
                    <span class="color-text">@Model.SpaceTypeName</span>
                </div>
                <div>
                    <label>BCAClassification:</label>
                    <span class="color-text">@Model.BCAClassificationName</span>
                </div>
                <div>
                    <label>Lamp Ballast Combination :</label>
                    <span class="color-text">@Model.LampBallastCombinationName</span>
                </div>
                <div>
                    <label>HVAC A/C :</label>
                    <span class="color-text">@Model.HVACAC</span>
                </div>
                <div>
                    <label>Quantity:</label>
                    <span class="color-text">@Model.Quantity</span>
                </div>
                <div>
                    <label>LampPower:</label>
                    <span class="color-text">@Model.NominalLampPower</span>
                </div>

            </div>
            <div class="col-md-6">

                @if (Model.BaselineUpgrade == 1)
                {
                    <div>
                        <label>BaselineAssetLifetimeReference:</label>
                        <span class="color-text">@Model.BaselineAssetLifetimeReferenceName</span>
                    </div>
                    if (Model.lstVEECNonJ6Scenario != null)
                    {
                        foreach (var item in Model.lstVEECNonJ6Scenario)
                        {
                            <div>
                                <label>@item.ProjectForm :</label>
                                <span class="color-text">@item.Scenario</span>
                            </div>
                        }
                    }

                }
                else
                {
                    <div>
                        <label>UpgradeAssetLifetimeReference:</label>
                        <span class="color-text">@Model.UpgradeAssetLifetimeReferenceName</span>
                    </div>
                    if (Model.lstVEECNonJ6Scenario != null)
                    {
                        foreach (var item in Model.lstVEECNonJ6Scenario)
                        {
                            <div>
                                <label>@item.ProjectForm :</label>
                                <span class="color-text">@item.Scenario</span>
                            </div>
                        }
                    }
                }

               

            </div>

        </div>


    </div>



</div>