        @*SaveScheduleOnDropAndInsertEdit(startDate, startTime, endDate, endTime, label, detail, userId, jobId, jobSchedulingID, isNotification, status, false, userName, "", jobTitle, '@Model.IsCheckListView.ToString().ToLower()')*@
