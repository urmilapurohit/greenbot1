<script>
    var LoggedInNameLength = '@FormBot.Helper.ProjectSession.LoggedInName.Length';
    var usertypeId = '@ProjectSession.UserTypeId';
    var IsUserFSA= '@ProjectSession.IsUserFSA';
    $(document).ready(function () {
        debugger;
        if (LoggedInNameLength > 27)
            LoggedInNameLength = 27;
        $('#lblchangeuser').css('right', ((LoggedInNameLength * 5) + 115) + 'px');
        $('#SearchIcon').css('right', ((LoggedInNameLength * 5) + 205) + 'px');
        $('#Searchdiv').css('right', ((LoggedInNameLength * 5) + 215) + 'px');
        if (usertypeId == 1 || usertypeId == 3) {
            $('#lblhelp').css('right', ((LoggedInNameLength * 5) + 255) + 'px');
            $('#btnHelpFormBot').css('right', ((LoggedInNameLength * 5) + 285) + 'px');
        }
        else if (IsUserFSA !='False') {
            $('#lblhelp').css('right', ((LoggedInNameLength * 5) + 225) + 'px');
            $('#btnHelpFormBot').css('right', ((LoggedInNameLength * 5) + 255) + 'px');
        }
        else {
            $('#lblhelp').css('right', ((LoggedInNameLength * 5) + 115) + 'px');
            $('#btnHelpFormBot').css('right', ((LoggedInNameLength * 5) + 145) + 'px');
        }
    });

    function updateNotificationCount() {

        $.ajax({
            url: urlGetNotificationCount,
            type: "GET",
            dataType: "json",
            async: false,
            success: function (data) {
                $("#notificationCount").html(data);
            }
        });
    }

    $("#notificationDiv").click(function () {

        $.ajax({
            url: urlGetNotificationList,
            type: "GET",
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            success: function (data) {
                $('#popupboxNotification').modal("show");
                $("#notificationData").html(data);
            },
            error: function () {
                alert("Error Getting Notification.");
            }
        });
    });

    $("#btnClosepopupboxNotification").click(function () {
        $('#popupboxNotification').modal("hide");

        var notificationIds = [];

        $("#notesNotication li").each(function () {
            notificationIds.push($(this).attr("notification-id"));
        });

        $.ajax({
            url: urlRemoveNotification,
            type: "GET",
            datatype: "json",
            data: {notificationIds: notificationIds.toString()},
            success: function (data) {
                $("#notificationCount").html("0");
            },
            error: function (error) {
                alert("Somethign wrong in clearing notification")
            }
        });
    });

    updateNotificationCount();
    setInterval(updateNotificationCount, 60000);
</script>
