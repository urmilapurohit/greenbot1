    if (TempData.ContainsKey(SystemEnums.TempDataKey.IsProfile.ToString()))
    {
        ((List<FormBot.Main.Controllers.MenuIdList>)TempData[SystemEnums.TempDataKey.IsProfile.ToString()]).ForEach(d =>
        {
            if (d.MenuId == (int)SystemEnums.MenuId.profile.GetHashCode()) { isProfile = true; }

